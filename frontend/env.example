# InteractHub Frontend Environment Configuration
# Copy this file to .env and update the values

# API Service URLs
REACT_APP_ADMIN_SERVICE_URL=http://localhost:8081/api/admin
REACT_APP_MANAGER_SERVICE_URL=http://localhost:8083/api/manager
REACT_APP_CHAT_SERVICE_URL=http://localhost:8085/api/chat
REACT_APP_NOTIFICATION_SERVICE_URL=http://localhost:8084/api/notify

# WebSocket URL
REACT_APP_WEBSOCKET_URL=http://localhost:8085/ws

# Application Configuration
REACT_APP_APP_NAME=InteractHub
REACT_APP_VERSION=1.0.0

# Authentication Configuration
REACT_APP_TOKEN_KEY=interacthub_token
REACT_APP_USER_KEY=interacthub_user

# Development Configuration
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug






